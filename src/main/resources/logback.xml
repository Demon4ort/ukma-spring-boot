<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <include resource="org/springframework/boot/logging/logback/base.xml"/>
    <springProfile name="dev">
        <logger name="com.example.ukmaspringboot.controllers" level="DEBUG" additivity="false">
            <appender-ref ref="CONSOLE"/>
        </logger>
        >
    </springProfile>
    <springProfile name="prod">
        <logger name="com.example.ukmaspringboot.service" level="WARN" additivity="false">
            <appender-ref ref="FILE"/>
        </logger>
    </springProfile>
</configuration>